{
    "name": "pagevamp/laravel-cloudwatch-logs",
    "description": "Laravel Adapter for AWS CloudWatch",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Naren",
            "email": "developer.naren@gmail.com"
        }
    ],
    "autoload" : {
      "psr-4" : {
          "Pagevamp\\" : "src/",
          "Tests\\": "tests/"
      }
    },
    "require": {
        "maxbanton/cwh": "^1.1",
        "illuminate/support": "^5.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pagevamp\\Providers\\CloudWatchServiceProvider"
            ]

        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.2"
    }
}
